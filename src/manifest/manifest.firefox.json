{
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "{5a110dee-fb42-44a1-b64d-d0444a199f96}",
      "update_url": "https://storedsafe.com/addons/firefox/updates.json",
      "strict_min_version": "79.0"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      },
      "description": "Open StoredSafe Popup"
    },
    "fill": {
      "suggested_key": {
        "default": "Ctrl+Alt+F"
      },
      "description": "Fill form on page if data is available"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "StoredSafe",
    "default_popup": "popup.html",
    "default_icon": {
      "48": "assets/icon-inactive_48.png",
      "96": "assets/icon-inactive_96.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}
