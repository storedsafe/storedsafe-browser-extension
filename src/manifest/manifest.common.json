{
  "name": "StoredSafe",
  "version": "1.0.0",
  "description": "StoredSafe browser integration",
  "default_locale": "en",
  "icons": {
    "48": "assets/icon_48.png",
    "96": "assets/icon_96.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["externals/browser-polyfill.min.js", "content.js"]
    }
  ],
  "permissions": ["storage", "idle", "activeTab", "tabs", "alarms"]
}
