{
  "manifest_version": 2,
  "name": "StoredSafe",
  "version": "1.0",

  "description": "Password StoredSafe browser extension",
  "icons": {
    "64": "favicon.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["background.bundle.js"]
    }
  ]
}
