{
  "name": "storedsafe-browser-extension",
  "private": true,
  "version": "0.0.0-1",
  "description": "Browser extension for interfacing with Password StoredSafe.",
  "main": "src/extension/index.jsx",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext js --ext jsx",
    "lint:style": "stylelint **/*.scss",
    "clean": "gulp clean",
    "build": "NODE_ENV=production gulp",
    "build:dev": "NODE_ENV=development gulp",
    "build:watch": "NODE_ENV=development gulp watch"
  },
  "keywords": [
    "browser-extension",
    "react"
  ],
  "author": "Oscar Mattsson",
  "license": "AGPL-3.0-or-later",
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/extension/setup_tests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@svgr/webpack": "^5.3.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-merge-json": "^2.1.0",
    "gulp-rename": "^2.0.0",
    "jest": "^25.1.0",
    "react-axe": "^3.4.1",
    "react-test-renderer": "^16.13.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "vinyl-named": "^1.1.0",
    "webextension-polyfill": "^0.6.0",
    "webpack": "^4.42.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "storedsafe": "git+ssh://git@git.xpd.se:19922/oscar/storedsafe-javascript.git"
  }
}
